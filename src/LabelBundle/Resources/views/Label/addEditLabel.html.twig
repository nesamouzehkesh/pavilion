{% form_theme form '::twig/formRow.html.twig' %}

{% set twig_show_search = false %}
{% extends "LabelBundle::layout.html.twig" %}

{% block body %}
    {{ form_start(form) }}
        {{ form_errors(form) }}

        {{ form_row(form.title) }}
        {{ form_row(form.description) }}
        {{ form_row(form.save) }}
    {{ form_end(form) }}      
{% endblock %}