{% set title = title|trans %}
<!DOCTYPE html>
<html class="ng-scope" ng-app="myApp">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
        <meta name="robots" content="noindex,nofollow" />
        <title>{% block title %}{{ title }}{% endblock %}</title>
        {% include "::_asset.html.twig" %}
    </head>
    <body>
        <div class="container">
            {% include '::web/_navbar.html.twig' with {'isSecond': true} %}
                        
            <div class="row margin-top-20">
                <div class="col-sm-9">
                    {% include '::web/user/_navigator.html.twig' %}
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="page-header">
                                <h3>{% block pageTitle %}{{ title }}{% endblock %}</h3>
                            </div>
                            {% block pageBody %}
                                {% if content is defined %}
                                    <p>{{ content|raw }}</p>
                                {% else %}
                                    {{ 'Whoops! Page not found'|warningAlert }}
                                {% endif %}
                            {% endblock %}
                        </div>
                    </div>
                </div> 
            </div>
        </div>
    </body>
</html>