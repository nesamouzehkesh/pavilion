{% use "::base_body.html.twig" %}
<!DOCTYPE html>
<html class="ng-scope" ng-app="myApp">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
        <meta name="robots" content="noindex,nofollow" />
        <title>{% block title %}{% endblock %}</title>
        {% include "::_asset.html.twig" %}
        {% block htmlHeader %}{% endblock %}
    </head>
    <body>
        {% include '::_navbar.html.twig'%}
        <div class="container">
            <div class="row">
                <div class="col-sm-3">
                    {% include '::_sidebar.html.twig'%}
                </div>  
                <div class="col-sm-9">
                    <div id="flashBag-container" data-url="{{ path('display_flash_bag') }}"></div>
                                                
                    {% block pageOpenContainer %}{{ parent() }}{% endblock %}
                                
                    {% block flashMessageContainer %}{{ parent() }}{% endblock %}
                    
                    {% block breadcrumbContainer %}{{ parent() }}{% endblock %}

                    {% block headerContainer %}{{ parent() }}{% endblock %}

                    {% block bodyContainer %}{{ parent() }}{% endblock %}

                    {% block footerContainer %}{{ parent() }}{% endblock %}

                    {% block pageCloseContainer %}{{ parent() }}{% endblock %}
                </div> 
            </div>
        </div>
        {% block script %}{% endblock %}    
    </body>
</html>